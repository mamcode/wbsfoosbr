<?xml version="1.0" encoding="utf-8"?>
<odoo>
    	<record id="scan_form_dev_wizard" model="ir.ui.view">
			<field name="name">Scan Product</field>
			<field name="model">wizard.scan.dev.serial</field>
			<field name="arch" type="xml">
				<form string="Scan Serials" version="10.0">
					<group>
						<field name="serial"/>
					</group>
					<footer>
						<button string="Save >>" class="oe_highlight" type="object" name="save_next"/>
						-- or --
						<button string="Cancel" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>
</odoo>
